{
  "name": "regina-cli",
  "version": "1.1.0",
  "description": "Regina CLI for creating portifolios to backend developers.",
  "author": "davi-canuto @davi-canuto",
  "bin": {
    "regina": "./bin/run"
  },
  "homepage": "https://github.com/davi-canuto/regina-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "davi-canuto/regina-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@inquirer/select": "^1.2.9",
    "@oclif/command": "^1.8.36",
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^3.2.0",
    "axios": "^1.5.0",
    "dotenv": "^16.3.1",
    "fs": "^0.0.1-security",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.4.4",
    "@types/chai": "^4",
    "@types/js-yaml": "^4.0.5",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.40",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3.11.3",
    "prettier": "^3.0.3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.1",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "regina",
    "dirname": "regina",
    "commands": "./dist/commands",
    "topicSeparator": " ",
    "topics": {
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "build": "shx rm -rf dist && tsc -b",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "format": "prettier --write 'src/**/*.ts'",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/davi-canuto/regina-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
